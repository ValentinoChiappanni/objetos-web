---
import CardSecciones from './CardSecciones.astro'
import WatchVideo from './WatchVideo.astro'

const { videos } = Astro.props
---

<CardSecciones
	title="Videos"
	id="videos">
	{
		videos.map((video) => (
			<WatchVideo
				title={video.nombre}
				url={video.url}
				urlYoutube={video.urlYoutube}
			/>
		))
	}
</CardSecciones>
