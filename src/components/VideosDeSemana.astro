---
import CardSecciones from './CardSecciones.astro'
import WatchVideo from './WatchVideo.astro'

const { videos } = Astro.props
---

<CardSecciones>
	<h2
		id="videos"
		class="ml-3">
		Videos
	</h2>
	{
		videos.map((video) => (
			<WatchVideo
				title={video.nombre}
				url={video.url}
				urlYoutube={video.urlYoutube}
			/>
		))
	}
</CardSecciones>

<style>
	h3 {
		margin-bottom: 50px !important;
	}
</style>
